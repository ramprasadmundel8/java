import java.util.Scanner;

class bank 
{
    public String name, acc;
    public double intrate, bal;

    public void getdata() 
    {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter Name: ");
        name = scanner.nextLine();
        System.out.print("Enter Account Number: ");
        acc = scanner.nextLine();
        System.out.print("Enter Balance: ");
        bal = scanner.nextDouble();
        System.out.println();

    }

    public void printdata() 
    {
        System.out.println("Name: " + name);
        System.out.println("Account Number: " + acc);
        System.out.println("Balance: " + bal);
        System.out.println();
    }

    public void interest() 
    {
        System.out.println();
    }
}

class SBIbank extends bank
{
    public void interest() 
    {
        intrate = 0.06;
        System.out.println("The Interest rate is: " + intrate);
        System.out.println("The Interest earned is: " + (intrate*bal));
        System.out.println();
    }
}

class Citybank extends bank
{
    public void interest() 
    {
        intrate = 0.05;
        System.out.println("The Interest rate is: " + intrate);
        System.out.println("The Interest earned is: " + (intrate*bal));
        System.out.println();
    }
}

class Canarabank extends bank
{
    public void interest() 
    {
        intrate = 0.09;
        System.out.println("The Interest rate is: " + intrate);
        System.out.println("The Interest earned is: " + (intrate*bal));
        System.out.println();
    }
}

public class Inheritance 
{
    public static void main(String[] args) 
    {
        Scanner scanner = new Scanner(System.in);
        SBIbank c2 = new SBIbank();
        Citybank c3 = new Citybank();
        Canarabank c4 = new Canarabank();

        System.out.println("Welcome to SBI bank");
        c2.getdata();
        c2.printdata();
        c2.interest();

        System.out.println("Welcome to City bank");
        c3.getdata();
        c3.printdata();
        c3.interest();
        
        System.out.println("Welcome to Canara bank");
        c4.getdata();
        c4.printdata();
        c4.interest();

    }
}
